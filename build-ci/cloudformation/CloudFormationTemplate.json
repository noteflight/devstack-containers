{
  "Resources": {
    "BuildBucket": {
      "Type": "AWS::S3::Bucket"
    },
    "Repositorydynamodblocal": {
      "Type": "AWS::ECR::Repository",
      "Properties": {
        "RepositoryName": "nf-devstack-dynamodblocal"
      }
    },
    "CloudWatchLogGroupdynamodblocal": {
      "Type": "AWS::Logs::LogGroup",
      "Properties": {
        "LogGroupName": "/aws/codebuild/devstack-dynamodblocal",
        "RetentionInDays": 180
      }
    }
  },
  "Outputs": {
    "BuildBucket": {
      "Description": "The bucket containing build results and logs",
      "Value": {
        "Ref": "BuildBucket"
      }
    },
    "BuildBucketArn": {
      "Description": "The arn of the bucket containing build results and logs",
      "Value": {
        "Fn::Join": [
          "",
          [
            "arn:aws:s3:::",
            {
              "Ref": "BuildBucket"
            }
          ]
        ]
      }
    }
  }
}
