{
  "name": "devstack-dynamodblocal",
  "description": "Builds the devstack-dynamodblocal Docker image and pushes it to the nf-devstack-dynamodblocal ECR repository",
  "source": {
    "type": "GITHUB",
    "location": "https://github.com/noteflight/devstack-containers.git",
    "gitCloneDepth": 1,
    "buildspec": "build-ci/codebuild/buildspec-dynamodblocal.yml"
  },
  "sourceVersion": "main",
  "artifacts": {
    "type": "NO_ARTIFACTS"
  },
  "cache": {
    "type": "NO_CACHE"
  },
  "environment": {
    "type": "LINUX_CONTAINER",
    "image": "aws/codebuild/standard:4.0",
    "imagePullCredentialsType": "CODEBUILD",
    "computeType": "BUILD_GENERAL1_SMALL",
    "privilegedMode": true
  },
  "timeoutInMinutes": 60,
  "logsConfig": {
    "cloudWatchLogs": {
      "status": "ENABLED",
      "groupName": "/aws/codebuild/devstack-dynamodblocal"
    },
    "s3Logs": {
      "status": "ENABLED",
      "encryptionDisabled": true,
      "location": "arn:aws:s3:::nf-devstack-containers/build-log"
    }
  },
  "serviceRole": "arn:aws:iam::915434789528:role/codebuild-nf-devstack-containers"
}
